{
  "name": "lucid-router",
  "version": "1.2.1",
  "description": "a simple html5-history aware router",
  "main": "dist/router.js",
  "scripts": {
    "lint": "standard src/*",
    "test": "karma start",
    "build": "npm test && npm run build-router && npm run build-link",
    "build-router": "babel src/router.js --out-file dist/router.js --stage 0",
    "build-link": "babel src/link.js --out-file dist/link.js --stage 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spicydonuts/lucid-router.git"
  },
  "keywords": [
    "lucid",
    "router",
    "routing",
    "history",
    "html5",
    "isomorphic",
    "universal",
    "react"
  ],
  "author": "Michael Trotter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spicydonuts/lucid-router/issues"
  },
  "homepage": "https://github.com/spicydonuts/lucid-router",
  "dependencies": {
    "url-pattern": "0.10.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "eslint-plugin-flowtype": "^1.0.0",
    "isparta": "^3.0.4",
    "istanbul": "^0.3.20",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "jspm": "^0.16.6",
    "karma": "^0.13.10",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-jspm": "^2.0.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "phantomjs": "^1.9.18",
    "standard": "^5.3.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "url-pattern": "npm:url-pattern@0.10.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "eslint-plugin-flowtype"
    ]
  }
}
